      {
        "command": "quickTranslate.translateSelection",
        "title": "Quick Translate: Translate Selection",
        "category": "Quick Translate"
      },
      {
        "command": "quickTranslate.translateWithoutNormalization",
        "title": "Quick Translate: Translate Without Normalization",
        "category": "Quick Translate"
      },
      {
        "command": "quickTranslate.clearLastTranslation",
        "title": "Quick Translate: Clear Last Translation",
        "category": "Quick Translate"
      },
      {
        "command": "quickTranslate.openSettings",
        "title": "Quick Translate: Open Settings",
        "category": "Quick Translate"
      },
      {
        "command": "quickTranslate.copyLastTranslation",
        "title": "Quick Translate: Copy Last Translation",
        "category": "Quick Translate"
      },
      {
        "command": "quickTranslate.normalizationPreview",
        "title": "Quick Translate: Normalization Preview",
        "category": "Quick Translate"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "quickTranslate.translateSelection",
          "group": "navigation@99"
        }
      ],
      "commandPalette": [
        {
          "command": "quickTranslate.translateSelection"
        },
        {
          "command": "quickTranslate.translateWithoutNormalization"
        },
        {
          "command": "quickTranslate.clearLastTranslation"
        },
        {
          "command": "quickTranslate.openSettings"
        },
        {
          "command": "quickTranslate.copyLastTranslation"
        },
        {
          "command": "quickTranslate.normalizationPreview"
        }
      ]
    },
    "keybindings": [
      {
        "command": "quickTranslate.translateSelection",
        "key": "ctrl+shift+t",
        "mac": "cmd+shift+t",
        "when": "editorHasSelection && editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "publish:vsce": "pnpm dlx vsce publish --no-dependencies",
    "publish:ovsx": "pnpm dlx ovsx publish --no-dependencies",
    "vscode:publish": "npm-run-all publish:*",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "test:translate": "node --test --import tsx src/test/translate.test.ts"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "~20.19.33",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.12",
    "eslint": "^9.39.2",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
