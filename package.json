{
  "name": "vscode-quick-translate",
  "displayName": "VSCode Quick Translate",
  "description": "Translate selected or entered text with inline hover results. Optional normalization for identifiers (camelCase, snake_case, etc.). Copy last translation, normalization preview, and quick settings. Uses Google Translate; no API key required.",
  "version": "0.1.4",
  "publisher": "mewisme",
  "engines": {
    "vscode": "^1.84.0"
  },
  "license": "MIT",
  "author": "Nguyen Mau Minh",
  "repository": {
    "type": "git",
    "url": "https://github.com/mewisme/vscode-quick-translate"
  },
  "keywords": [
    "translate",
    "translation",
    "google translate",
    "hover",
    "normalization",
    "localization"
  ],
  "icon": "images/icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:quickTranslate.translateSelection",
    "onCommand:quickTranslate.translateWithoutNormalization",
    "onCommand:quickTranslate.clearLastTranslation",
    "onCommand:quickTranslate.openSettings",
    "onCommand:quickTranslate.copyLastTranslation",
    "onCommand:quickTranslate.normalizationPreview"
  ],
